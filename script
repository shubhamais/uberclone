			<b:case value='none'/>
				<style type="text/css">body.static_page #content{width:100%;float:none}body.static_page #main-sidebar{display:none}</style>
			<b:case value='left'/>
				<style type="text/css">body.static_page #content{float:right;}body.static_page #main-sidebar{float:left}body.static_page #main-sidebar #sidebar.abs-stay,body.static_page #main-sidebar #sidebar.abs-bot {right: auto!important;left: 0!important;}</style>
			<b:case value='right'/>
				<style type="text/css">body.static_page #content{float:left;}body.static_page #main-sidebar{float:right}</style>
			<b:default />
		</b:switch>
	</b:with>

	<!--item-sidebar-manager--><b:with value='&quot;default&quot;' var='item_sidebar'><!--item-sidebar-manager-->
