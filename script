				<style type="text/css">body.item #content{float:left;}body.item #main-sidebar{float:right}</style>
			<b:default />
		</b:switch>
	</b:with>
	</b:if>

	<!--index-sidebar-manager--><b:with value='&quot;default&quot;' var='index_sidebar'><!--index-sidebar-manager-->
