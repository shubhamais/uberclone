		<b:if cond='false'><!--amp-enabled-start--></b:if><b:if cond='false and data:blog.pageType in {&quot;static_page&quot;,&quot;item&quot;} and data:blog.isMobileRequest'><b:if cond='false'><!--amp-enabled-end--></b:if>
		<b:else/>
			<b:if cond='data:blog.pageType == &quot;item&quot; or data:blog.url == data:blog.homepageUrl'>
				<b:section id='content-section-after-post' expr:name='data:blog.url != data:blog.homepageUrl? &quot;&quot; : &quot;After Post Body Content&quot;'>				
				</b:section>
			</b:if>
			<b:section id='content-section-wide-bottom' expr:name='data:blog.url != data:blog.homepageUrl? &quot;&quot; : &quot;All Pages Content Bottom&quot;'></b:section>
		<!-- end of AMP check-->
		</b:if>

	</div><!--end #content-->

	

	<!--home-sidebar-manager--><b:with value='&quot;default&quot;' var='home_sidebar'><!--home-sidebar-manager-->
		<b:if cond='data:blog.pageType != &quot;item&quot;'><!--prevent preview pages-->
			<b:switch var='data:home_sidebar'>
				<b:case value='none'/>
					<style type="text/css">.wrapper.home #content{width:100%;float:none}.wrapper.home #main-sidebar{display:none}</style>
				<b:case value='left'/>
					<style type="text/css">.wrapper.home #content{float:right;}.wrapper.home #main-sidebar{float:left} .wrapper.home #main-sidebar #sidebar.abs-stay, .wrapper.home #main-sidebar  #sidebar.abs-bot {right: auto!important;left: 0!important;}</style>
				<b:case value='right'/>
					<style type="text/css">.wrapper.home #content{float:left;}.wrapper.home #main-sidebar{float:right}</style>
				<b:default />
			</b:switch>
		</b:if>
	</b:with>

	<b:if cond='false'><!--amp-enabled-start--></b:if><b:if cond='false and data:blog.pageType in {&quot;static_page&quot;,&quot;item&quot;} and data:blog.isMobileRequest'><b:if cond='false'><!--amp-enabled-end--></b:if>
	<b:else/>

	<!--static_page-sidebar-manager--><b:with value='&quot;default&quot;' var='static_page_sidebar'><!--static_page-sidebar-manager-->
